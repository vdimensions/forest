<?xml version="1.0"?>
<package >
  <metadata>
    <id>${id}-Debug</id>
    <title>${title}-Debug</title>
    <version>${version}</version>
    <description>${id} debug symbols</description>
    <summary>${id} debug symbols</summary>
    <releaseNotes></releaseNotes>
    <tags>${id}-Debug Debug</tags>
    <!--
    <projectUrl>https://nuget.org/packages/Axle.Forest.Web.Client</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
     -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <authors>${company}</authors>
    <owners>${company}</owners>
    <copyright>${copyright}</copyright>
    <licenseUrl>${license_url}</licenseUrl>
    <dependencies>
      <dependency id="Forest" version="${version}" />
    </dependencies>
    <frameworkAssemblies>
    </frameworkAssemblies>
    <references></references>
  </metadata>
  <files>
    <file src="bin\*\net45\\${id}.pdb" target="lib\net45" />
    <file src="bin\*\net45\\${id}.mdb" target="lib\net45" />

    <file src="bin\*\net40\\${id}.pdb" target="lib\net40" />
    <file src="bin\*\net40\\${id}.mdb" target="lib\net40" />

    <file src="bin\*\net40-client\\${id}.pdb" target="lib\net40-client" />
    <file src="bin\*\net40-client\\${id}.mdb" target="lib\net40-client" />

    <file src="bin\*\net35\\${id}.pdb" target="lib\net35" />
    <file src="bin\*\net35\\${id}.mdb" target="lib\net35" />

    <file src="bin\*\net35-Unity Full v3.5\\${id}.pdb" target="lib\net35-Unity Full v3.5" />
    <file src="bin\*\net35-Unity Full v3.5\\${id}.mdb" target="lib\net35-Unity Full v3.5" />

    <file src="bin\*\net35-Unity Subset v3.5\\${id}.pdb" target="lib\net35-Unity Subset v3.5" />
    <file src="bin\*\net35-Unity Subset v3.5\\${id}.mdb" target="lib\net35-Unity Subset v3.5" />

    <file src="bin\*\net35-Unity Micro v3.5\\${id}.pdb" target="lib\net35-Unity Micro v3.5" />
    <file src="bin\*\net35-Unity Micro v3.5\\${id}.mdb" target="lib\net35-Unity Micro v3.5" />

    <file src="bin\*\net35-Unity Web v3.5\\${id}.pdb" target="lib\net35-Unity Web v3.5" />
    <file src="bin\*\net35-Unity Web v3.5\\${id}.mdb" target="lib\net35-Unity Web v3.5" />

    <file src="bin\*\net20\\${id}.pdb" target="lib\net20" />
    <file src="bin\*\net20\\${id}.mdb" target="lib\net20" />

    <file src="**\*.ExternalAnnotations.xml" exclude="**\*.ExternalAnnotations.xml" />
  </files>
</package>

